{
  "name": "electron-browser",
  "version": "1.0.0",
  "description": "File browser using electron",
  "main": "index.js",
  "scripts": {
    "start": "electron app",
    "debug": "gulp debug-electron",
    "build": "webpack --config webpack.config.dev.js && webpack --config webpack.config.back.js",
    "watch:test": "webpack --config webpack.config.test.js --watch",
    "run:test": "karma start",
    "backend:test": "jasmine --config=test/jasmine.json",
    "test": "npm-run-all -p -r watch:test run:test"
  },
  "keywords": [
    "electron",
    "browser",
    "angular"
  ],
  "author": "tonirturro@outlook.es",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.6.8",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "electron-connect": "^0.6.3",
    "eslint": "^4.13.1",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "angular": "^1.6.8",
    "async": "^2.6.0",
    "lunr": "^2.1.5",
    "osenv": "^0.1.4"
  }
}
